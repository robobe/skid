{
    "files.associations": {
        "*.urdf": "xml",
        "*.sdf": "xml",
        "iostream": "cpp",
        "*.ipp": "cpp"
    },
    "cSpell.ignoreWords": [
        "cap",
        "cap gstreamer",
        "gstreamer"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true
    }
}